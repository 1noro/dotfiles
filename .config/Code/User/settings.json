{
    "editor.fontFamily": "'JetBrainsMono Nerd Font', 'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "workbench.startupEditor": "newUntitledFile",
    "editor.renderWhitespace": "boundary",
    "editor.multiCursorModifier": "ctrlCmd",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "editor.fontSize": 18,
    "editor.mouseWheelZoom": true,
    "[go]": {
        "editor.formatOnSave": false
    },
    "editor.suggestSelection": "first",
    "editor.rulers": [
        80,
        100
    ],
    "cSpell.language": "es-ES,en-GB",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "tabnine.experimentalAutoImports": true,
    "workbench.iconTheme": "material-icon-theme",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.minimap.enabled": false,
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "latex-workshop.view.pdf.viewer": "tab",
    "[latex]": {
        "editor.wordWrap": "on"
    },
    "cSpell.enableFiletypes": [
        "dockerfile",
        "makefile"
    ],
    "editor.cursorStyle": "block",
    "files.insertFinalNewline": true,
    "vim.useSystemClipboard": true,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": true,
        "json": true,
        "latex": false
    },
    "redhat.telemetry.enabled": false,
    "workbench.editor.untitled.hint": "hidden",
    "workbench.colorTheme": "Gruvbox Dark Medium",
    "jenkins.pipeline.linter.connector.crumbUrl": "http://192.168.110.109:8080/crumbIssuer/api/xml?xpath=concat(//crumbRequestField,%22:%22,//crumb)",
    "jenkins.pipeline.linter.connector.url": "http://192.168.110.109:8080/pipeline-model-converter/validate",
    "jenkins.pipeline.linter.connector.user": "frank",
    "jenkins.pipeline.linter.connector.strictssl": false,
    "jenkins.pipeline.linter.connector.token": "112de9c521346ff60876f31dcbbbf20bfc",
    "cSpell.userWords": [
        "autogestionado",
        "mahjong",
        "mkdir",
        "nginx"
    ],
    "telemetry.telemetryLevel": "crash",
    "liveshare.accessibility.soundsEnabled": false,
    "liveshare.accessibility.voiceEnabled": false,
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "shellcheck.customArgs": [
        "-x"
    ],
    "window.zoomLevel": 1
}
